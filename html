  <h1> Out of Office </h1>
  </div>

    <div id="map" style="width: 500px; height: 400px;"></div>

    <script type="text/javascript">
      var locations = [
        ['Ozone Coffee', 51.524879, -0.086834, 5],
        ['FuckCoffee/Jamestown Coffee', 51.527665, -0.067903, 4],
        ['The Hoxton', 51.525650, -0.082731, 3],
        ['Pitfield London', 51.527419, -0.083782, 2],
        ['The Brew, Eagle House', 51.527988, -0.090524, 1],
        ['Fix 126', 51.526177, -0.0802644, 6],
        ['Kahaila Cafe', 51.523963, -0.071534],
        ['Bishopsgate Institue', 51.518768, -0.079376]
      ];

      var map = new google.maps.Map(document.getElementById('map'), {
        zoom: 13,
        center: new google.maps.LatLng(51.525575, -0.070789),
        mapTypeId: google.maps.MapTypeId.ROADMAP
      });

      var infowindow = new google.maps.InfoWindow();

      var marker, i;

      for (i = 0; i < locations.length; i++) {
        marker = new google.maps.Marker({
          position: new google.maps.LatLng(locations[i][1], locations[i][2]),
          map: map
        });

        google.maps.event.addListener(marker, 'click', (function(marker, i) {
          return function() {
            infowindow.setContent(locations[i][0]);
            infowindow.open(map, marker);
          }
        })(marker, i));
      }
    </script>

